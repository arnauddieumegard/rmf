<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         id="application"
         point="org.eclipse.core.runtime.applications">
      <application>
         <run
               class="org.eclipse.rmf.reqif10.headlessvalidator.Application">
         </run>
      </application>
   </extension>
   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            application="org.eclipse.rmf.reqif10.headlessValidator.application"
            name="Headless RMF Validator">
         <property
               name="appName"
               value="Headless RMF Validator">
         </property>
      </product>
   </extension>

</plugin>
